{
  "extends": [
    "google",
    "prettier",
    "plugin:@typescript-eslint/recommended",
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "prettier",
    "@typescript-eslint",
  ],
  "rules": {
    "no-unused-vars": "warn",
    "prefer-const": "warn",
    "eqeqeq": "warn",
    "require-jsdoc": "warn"
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
  },
  "env": {
    "es6": true
  }
}
